// import { useMovieData } from '@mui/x-data-grid-generator';

const columns_name = [

// field: "title"
// groupable: false
// headerName: "Title"
// width: 200

  { field: 'name',groupable: false, headerName: 'Name', width: 200 },
  { field: 'email',groupable: false, headerName: 'Email', width: 200 },
  { field: 'role',groupable: false, headerName: 'Role', width: 200 },
  { field: 'assigned',groupable: false, headerName: 'Assigned', width: 200 },
];

  const dummyUsers  = [

  {
